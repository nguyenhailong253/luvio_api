# Ref: https://stackoverflow.com/a/60865364/8749888 -- exclude django boilerplate files
[run]
branch = True
source = .
omit = ./venv/*,*/tests/*.py,*test*,*apps.py,*manage.py,*__init__.py,*/migrations/*.py,*asgi*,*wsgi*,*admin.py,*urls.py,*settings.py

[report]
omit = ./venv/*,*/tests/*.py,*test*,*apps.py,*manage.py,*__init__.py,*/migrations/*.py,*asgi*,*wsgi*,*admin.py,*urls.py,*settings.py